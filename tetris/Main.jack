/** Main class of tetris game. */
class Main {

    function void main() {
        var TetrisGame game;
        var boolean game_state;

        let game_state = true;
        let game = TetrisGame.new(0,0);


        while(game_state){
            // play the game
            do game.draw();            
            
        }

        do game.dispose();
        return;
    }
}