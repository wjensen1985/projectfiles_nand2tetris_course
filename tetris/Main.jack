/** Main class of tetris game. */
class Main {

    function void main() {
        var TetrisGame game;
        var boolean game_state;

        let game_state = true;
        let game = TetrisGame.new(0,0);

        do game.setup();

        while(game_state){
            // play the game
            let game_state = game.play();            
            
        }

        do game.dispose();
        return;
    }
}